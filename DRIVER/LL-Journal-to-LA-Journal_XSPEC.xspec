    <x:scenario label="Scenario for testing l grouping, lilabel and note creation under bodytext ">
        <x:context>
            <JOURNALDOC>
                <jrnl:body>
                    <jrnl:bodytext searchtype="ARTICLE">
                        <level>
                            <levelprelim>
                                <abstract>
                                    <p>
                                        <text>
                                            <emph typestyle="it">In this article, the authors consider the legal rules on security interests over bank accounts in China, drawing comparisons with the law in other jurisdictions particularly the UK.</emph>
                                        </text>
                                    </p>
                                </abstract>
                            </levelprelim>
                        </level>
                        <level>
                            <heading>
                                <title>
                                    <emph typestyle="bf">KEY POINTS</emph>
                                </title>
                            </heading>
                            <bodytext>
                                <l>
                                    <li>
                                        <lilabel> </lilabel>
                                        <p>
                                            <text>&#x25cf;     Conceptual difficulties under English law surrounding charge-backs do not present practical obstacles in China.</text>
                                        </p>
                                    </li>
                                </l>
                                <l>
                                    <li>
                                        <lilabel> </lilabel>
                                        <p>
                                            <text>&#x25cf;     Under Chinese law it is unclear whether bank accounts are receivables.</text>
                                        </p>
                                    </li>
                                </l>
                                <l>
                                    <li>
                                        <lilabel> </lilabel>
                                        <p>
                                            <text>&#x25cf;     It is questionable whether there is a registration requirement for fixed charges over bank accounts where the proceeds in the account relate to future receivables.</text>
                                        </p>
                                    </li>
                                </l>
                                <l>
                                    <li>
                                        <lilabel> </lilabel>
                                        <p>
                                            <text>&#x25cf;     A Chinese set-off right is not as protective as an English triple cocktail arrangement but it avoids the recharacterisation risk.</text>
                                        </p>
                                    </li>
                                </l>                                
                            </bodytext>
                        </level>
                    </jrnl:bodytext>
                </jrnl:body>
            </JOURNALDOC>
        </x:context>

        <x:expect label="l grouping, lilabel and bodytext-note transformed">
            <JOURNALDOC>
                <jrnl:body>
                    <jrnl:bodytext>
                        <level>
                            <heading>
                                <title>KEY POINTS</title>
                            </heading>
                            <bodytext>
                                <note>
                                    <l>
                                        <li>
                                            <lilabel>●</lilabel>
                                            <p>
                                                <text>Conceptual difficulties under English law surrounding charge-backs do not present practical obstacles in China.</text>
                                            </p>
                                        </li>
                                        <li>
                                            <lilabel>●</lilabel>
                                            <p>
                                                <text>Under Chinese law it is unclear whether bank accounts are receivables.</text>
                                            </p>
                                        </li>
                                        <li>
                                            <lilabel>●</lilabel>
                                            <p>
                                                <text>It is questionable whether there is a registration requirement for fixed charges over bank accounts where the proceeds in the account relate to future receivables.</text>
                                            </p>
                                        </li>
                                        <li>
                                            <lilabel>●</lilabel>
                                            <p>
                                                <text>A Chinese set-off right is not as protective as an English triple cocktail arrangement but it avoids the recharacterisation risk.</text>
                                            </p>
                                        </li>
                                    </l>
                                </note>                                
                            </bodytext>
                        </level>             
                    </jrnl:bodytext>
                </jrnl:body>
            </JOURNALDOC>            
        </x:expect>
    </x:scenario>


